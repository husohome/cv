    "import re\n",
    "import pandas as pd\n",
    "from datetime import datetime\n",
    "\n",
    "def parse_ym(text):\n",
    "    text = re.sub(r\"\\D\",\"\", text) #removing everything that isn't a digit\n",
    "    try:\n",
    "        result = datetime.strptime(text, \"%Y%m\")\n",
    "    except:\n",
    "        try:\n",
    "            result = datetime.strptime(text, \"%y%m\")\n",
    "        except:\n",
    "            try:\n",
    "                result = datetime.strptime(text, \"%Y\")\n",
    "            except:\n",
    "                try:\n",
    "                    result = datetime.strptime(text, \"%y\")\n",
    "                except:\n",
    "                    raise TypeError(\"Can't parse time! Currently the only formats accepted are yyyymm or yymm or yy or yyyy \")\n",
    "    return result\n",
    "\n",
    "def lex(text):\n",
    "    for statement in text.split('\\n'):\n",
    "        stripped_statement = statement.strip()\n",
    "        m_comment1 = re.search(r\"!([^\\r\\n]+)\", stripped_statement)\n",
    "        m_comment2 = re.search(r\"//([^\\r\\n]+)\", stripped_statement)\n",
    "        m_synonymous = re.match(r\"(([^\\r\\n]+)\\s*)(=\\s*([^\\r\\n]+))+\", stripped_statement)\n",
    "        m_constructive = re.match(r\"([^\\r\\n]+)\\s*=~(\\s*[^\\r\\n]+\\s*\\+)*(\\s*[^\\r\\n]+\\s*)\", stripped_statement)\n",
    "        m_title = re.match(r\"@([^\\r\\n]+)\", stripped_statement)\n",
    "       \n",
    "        if m_comment1:\n",
    "            stripped_statement = re.sub(r\"!([^\\r\\n]+)\", r\"\", stripped_statement)\n",
    "        \n",
    "        elif m_comment2:\n",
    "            stripped_statement = re.sub(r\"//([^\\r\\n]+)\", r\"\", stripped_statement)\n",
    "                               \n",
    "        elif m_constructive:\n",
    "            y, x = m_constructive.group(0).split(\"=~\")\n",
    "            c_list = [y.replace(\" \",\"\")] + [constructor for constructor in x.replace(\" \", \"\").split(\"+\")]\n",
    "            # the first element is the constructed, and the rest are the constructors\n",
    "            # if a =~ b + c, a is the constructed, and b, c are both constructors\n",
    "            yield \"constructive\", c_list\n",
    "            stripped_statement = re.sub(r\"([^\\r\\n]+)\\s*=~(\\s*[^\\r\\n]+\\s*\\+)*(\\s*[^\\r\\n]+\\s*)\", r\"\", stripped_statement)\n",
    "        \n",
    "        elif m_synonymous:\n",
    "            syns = m_synonymous.group(0).replace(\" \",\"\").split(\"=\")\n",
    "            s_dict = {'default':[], 'nondefault':[]}\n",
    "            for s in syns:\n",
    "                if \"*\" in s:\n",
    "                    s = s.replace(\"*\",\"\")\n",
    "                    s_dict['default'].append(s)\n",
    "                else:\n",
    "                    s_dict['nondefault'].append(s)\n",
    "            s_list = s_dict['default'] + s_dict['nondefault']\n",
    "            # this makes sure the highlighted expression always comes first\n",
    "            yield \"synonymous\", s_list\n",
    "            stripped_statement = re.sub(r\"(([^\\r\\n]+)\\s*)(=\\s*([^\\r\\n]+))+\", r\"\", stripped_statement)\n",
    "                               \n",
    "        elif m_title: \n",
    "            yield \"title\", m_title.group(1).strip()\n",
    "            stripped_statement = re.sub(r\"@([^\\r\\n]+)\", r\"\", stripped_statement)\n",
    "                   \n",
    "        for t in stripped_statement.split():\n",
    "            t = re.sub(r\"\\s+\", \" \",t)\n",
    "            m_money = re.match(r\"\\$(\\d+)[^\\r\\n]{0,6}(\\w\\wD)?\", t)\n",
    "            m_yyyymm = re.match(r\"(\\d{4,6})[-~]?(\\d{4,6})?\", t)\n",
    "            m_tags = re.match(r\"\\#([^\\r\\n]+)\", t)\n",
    "            \n",
    "            # self-defined categories\n",
    "            m_other_specs = re.match(r\"([^\\r\\n]+)::\", t)\n",
    "            if m_money: \n",
    "                yield \"money\", m_money.group(1).strip()\n",
    "                if m_money.group(2) is not None: \n",
    "                    yield \"money\", float(m_money.group(2).strip())\n",
    "            elif m_yyyymm:\n",
    "                yield \"start_ym\", parse_ym(m_yyyymm.group(1).strip())\n",
    "                if m_yyyymm.group(2) is not None:\n",
    "                    yield \"end_ym\", parse_ym(m_yyyymm.group(2).strip())\n",
    "                else:\n",
    "                    yield \"end_ym\", datetime.today()\n",
    "            elif m_tags: yield \"tag\", m_tags.group(1).strip()\n",
    "            elif m_other_specs: yield m_other_specs.group(1).strip(), \"\"\n",
    "            else:\n",
    "                yield \"content\", t.strip()\n",
    "\n",
    "list(lex(test_entry))"
   ]
